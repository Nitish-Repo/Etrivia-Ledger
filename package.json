{
  "name": "etrivia-ledger",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "ionic serve --host 0.0.0.0 --port 4204",
    "start:android:liveReload": "ionic cap run android --livereload --external --public-host=192.168.0.151",
    "start:ios:liveReload": "ionic cap run ios --livereload --external --public-host=192.168.0.151",
    "build": "ng build",
    "build:folder": "ng build --base-href /app/ --output-path www/app",
    "build:web:prod": "ng build --configuration production --base-href /app/ --output-path www/app",
    "build:web:dev": "ng build --configuration development --base-href /app/ --output-path www/app",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "ionic:serve:before": "npm run copy:sql:wasm",
    "copy:sql:wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm src/assets"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
